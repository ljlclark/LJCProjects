<items>
  <IsValidFolder>
    <summary>
      Checks for a valid source folder.
    </summary>
    <param name="folderName">The folder name.</param>
    <returns>True if valid, otherwise false.</returns>
  </IsValidFolder>
  <IsValidFile>
    <summary>
      Checks for a valid file.
    </summary>
    <param name="fileSpec">The File specification.</param>
    <returns>True if valid, otherwise false.</returns>
  </IsValidFile>
  <CreateBackup>
    <summary>
      Creates a backup file.
    </summary>
    <param name="filePath">The source file path.</param>
    <returns>The backup file path.</returns>
  </CreateBackup>
  <IsEqual>
    <summary>
      Do an Ignore Case string compare.
    </summary>
    <param name="stringA">The first string.</param>
    <param name="stringB">The second string.</param>
  </IsEqual>
</items>