<!-- UnitMeasureManager.xml -->
<items>
  <LoadWithCodes>
    <summary>
      Loads a collection of data records ordered by Description.
    </summary>
    <param name="categoryCode">The UnitCategory code.</param>
    <param name="systemCode">The UnitSystem code.</param>
    <returns>The Data Object collection.</returns>
  </LoadWithCodes>
  <LoadBySequence>
    <summary>
      Loads a collection of data records ordered by Sequence.
    </summary>
    <returns>The Data Object collection.</returns>
  </LoadBySequence>
  <ConvertUnit>
    <summary>
      Convert units.
    </summary>
    <param name="fromUnitMeasureID">The source UnitMeasure ID.</param>
    <param name="toUnitMeasureID">The target UnitMeasure ID.</param>
    <param name="value">The source value.</param>
    <returns>The converted value.</returns>
  </ConvertUnit>
</items>