<!-- GenTypeXml.xml -->
<items>
  <CreateTypeXml>
    <summary>
      Creates the Type XML Data values.
    </summary>
  </CreateTypeXml>
  <!-- Constructors -->
  <CreateTypeXmlC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="genRoot">The GenRoot generator object.</param>
    <param name="genAssembly">The GenAssembly generator object.</param>
    <param name="dataAssembly">The DataAssembly data object.</param>
    <param name="dataType">The DataType data object.</param>
    <param name="assemblyReflect">The LJCAssemblyReflect object.</param>
  </CreateTypeXmlC>
  <!-- Public Methods -->
  <GetXmlData>
    <summary>
      Returns the XML Data values.
    </summary>
    <returns>The XML Data values string.</returns>
  </GetXmlData>
  <!-- Private Methods -->
  <AddFields>
    <summary>
      Adds the Field data elements.
    </summary>
    <param name="section">The Section object.</param>
  </AddFields>
  <AddMethods>
    <summary>
      Adds the Methods data elements.
    </summary>
    <param name="section">The Section object.</param>
    <param name="usePublic">The include only Public methods flag.</param>
  </AddMethods>
  <AddProperties>
    <summary>
      Adds the Properties data elements.
    </summary>
    <param name="section">The Section object.</param>
  </AddProperties>
  <AddSyntax>
    <summary>
      Adds the Type syntax elements.
    </summary>
    <param name="replacements">The Replacements object.</param>
  </AddSyntax>
  <FieldCount>
    <summary>
      Gets the Field count.
    </summary>
    <returns>The Field count.</returns>
  </FieldCount>
  <MethodCount>
    <summary>
      Gets the Method count.
    </summary>
    <param name="usePublic">The include only Public methods flag.</param>
    <returns>The Method count.</returns>
  </MethodCount>
  <PropertyCount>
    <summary>
      Gets the Property count.
    </summary>
    <returns>The Property count.</returns>
  </PropertyCount>
  <SetExampleRemarks>
    <summary>
      The Example Remarks elements.
    </summary>
    <param name="section">The Section object.</param>
    <returns>True if remarks exist, otherwise false.</returns>
  </SetExampleRemarks>
  <SetTypeRemarks>
    <summary>
      Sets the Object Remarks elements.
    </summary>
    <param name="section">The Section object.</param>
    <returns>True if remarks exist, otherwise false.</returns>
  </SetTypeRemarks>
</items>