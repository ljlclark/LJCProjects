<!-- ColumnHTML.xml -->
<items>
  <!-- HTML Methods -->
  <DataTableHTML>
    <summary>
      Gets the HTML Table document from DataTable.
    </summary>
    <returns>The HTML document.</returns>
  </DataTableHTML>
  <GetHTML>
    <summary>
      Gets the HTML Table document.
    </summary>
    <param name="dataType"></param>
    <returns></returns>
  </GetHTML>
  <ResultHTML>
    <summary>
      Gets the HTML Table document from DbResult.
    </summary>
    <returns>The HTML document.</returns>
  </ResultHTML>
  <!-- Data Methods -->
  <GetDataTable>
    <summary>
      Gets the DataTable object.
    </summary>
    <returns>The DataTable object.</returns>
  </GetDataTable>
  <GetResult>
    <summary>
      Gets the DbResult object.
    </summary>
    <returns>The DbResult object.</returns>
  </GetResult>
</items>