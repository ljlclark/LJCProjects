<!-- ColumnHTML.xml -->
<items>
  <!-- Main Methods -->
  <ColumnHTMLDoc>
    <summary>
      Gets the HTML Table document.
    </summary>
    <param name="dataType"></param>
    <returns></returns>
  </ColumnHTMLDoc>
  <!-- HTML Methods -->
  <DataHTML>
    <summary>
      Creates the HTML Table document from a DataObject.
    </summary>
    <param name="dataObjects">The data objects collection.</param>
    <param name="heading">The page heading text.</param>
    <param name="propertyNames">The included property names.</param>
    <param name="textState">The TextState object.</param>"
    <returns>The HTML Table document.</returns>
  </DataHTML>
  <DataTableHTML>
    <summary>
      Creates the HTML Table document from DataTable.
    </summary>
    <param name="dataTable">The DataTable object.</param>
    <param name="heading">The page heading text.</param>
    <param name="textState">The TextState object.</param>"
    <returns>The HTML Table document.</returns>
  </DataTableHTML>
  <ResultHTML>
    <summary>
      Creates the HTML Table document from DbResult.
    </summary>
    <param name="dbResult">The DbResult object.</param>
    <param name="heading">The page heading text.</param>
    <param name="textState">The TextState object.</param>"
    <returns>The HTML Table document.</returns>
  </ResultHTML>
  <DataGridHTML>
    <summary>
      Creates the HTML Table document from a DataGridView.
    </summary>
    <param name="grid">The DataGridView object.</param>
    <param name="heading">The page heading text.</param>
    <param name="textState">The TextState object.</param>
    <returns>The HTML Table document.</returns>
  </DataGridHTML>
  <!-- Data Methods -->
  <GetDataTable>
    <summary>
      Gets the DataTable object.
    </summary>
    <returns>The DataTable object.</returns>
  </GetDataTable>
  <GetDataColumns>
    <summary>
      Gets the Data Object.
    </summary>
    <returns></returns>
  </GetDataColumns>
  <GetResult>
    <summary>
      Gets the DbResult object.
    </summary>
    <returns>The DbResult object.</returns>
  </GetResult>
</items>