<!-- GenDataManager.xml -->
<items>
  <GenDataManager>
    <summary>
      Provides GenData specific XML data manipulation methods.
    </summary>
  </GenDataManager>
  <GenDataManagerC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="fileSpec">The XML data file specification.</param>
  </GenDataManagerC>
  <AddSection>
    <summary>
      Adds a Section record to the object data.
    </summary>
    <param name="section">The Section object.</param>
  </AddSection>
  <RetrieveSection>
    <summary>
      Retrieves a Section record from the object data.
    </summary>
    <param name="sectionName">The Section name.</param>
    <returns>The Section object.</returns>
  </RetrieveSection>
  <LoadSections>
    <summary>
      Retrieves a collection of data records.
    </summary>
    <returns>The Sections collection.</returns>
  </LoadSections>
  <DeleteSection>
    <summary>
      Deletes the Section record from the object data.
    </summary>
    <param name="sectionName">The Section name.</param>
    <returns>True if successful, otherwise false.</returns>
  </DeleteSection>
  <Save>
    <summary>
      Save the XML data.
    </summary>
    <returns>True if successful, otherwise false.</returns>
  </Save>
</items>