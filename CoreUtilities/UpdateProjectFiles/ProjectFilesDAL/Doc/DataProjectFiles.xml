  <GetFileSpec>
    <summary>
      Create a File Spec from ProjectFile data.
    </summary>
    <param name="codeLineName">The CodeLine name.</param>
    <param name="codeGroupName">The CodeGroup name.</param>
    <param name="solutionName">The Solution name.</param>
