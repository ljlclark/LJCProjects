<items>
  <ViewJoinManager>
    <summary>
      Provides ViewJoin specific data manipulation methods.
    </summary>
  </ViewJoinManager>
  <ViewJoinManagerC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="dbServiceRef">The database service reference object.</param>
    <param name="dataConfigName">The data configuration name.</param>
    <param name="tableName">The table name.</param>
  </ViewJoinManagerC>
  <RetrieveWithID>
    <summary>
      Retrieve the record by ID.
    </summary>
    <param name="id">The ID value.</param>
    <returns>The data object record.</returns>
  </RetrieveWithID>
  <RetrieveWithUniqueKey>
    <summary>
      Retrieves the record by the unique key.
    </summary>
    <param name="viewDataID">The ViewData ID value.</param>
    <param name="joinTableName">The join table name value.</param>
    <returns>The data object record.</returns>
  </RetrieveWithUniqueKey>
  <LoadWithParentID>
    <summary>
      Retrieves a collection of data records for the specified parent ID.
    </summary>
    <param name="viewDataID">The parent ID.</param>
    <returns>The data object record collection.</returns>
  </LoadWithParentID>
  <SaveData>
    <summary>
      Updates the record if it exists, otherwise creates it.
    </summary>
    <param name="viewJoin">The data record.</param>
    <returns>True if successful otherwise, false.</returns>
  </SaveData>
  <AddData>
    <summary>
      Adds a data record.
    </summary>
    <param name="viewJoin">The data record.</param>
    <returns>The added data object record with the assigned ID value.</returns>
  </AddData>
</items>