<!-- ControlItems.xml -->
<items>
  <!-- Collection Methods -->
  <Add1>
    <summary>
      Adds the specified object.
    </summary>
    <param name="item">The item object.</param>
  </Add1>
  <Add2>
    <summary>
      Creates and adds the object with the provided values.
    </summary>
    <param name="propertyName">The Property Name.</param>
    <param name="id">The ID value.</param>
    <param name="description">The Description.</param>
  </Add2>
  <GetCollection>
    <summary>
      Get custom collection from List&lt;T&gt;.
    </summary>
    <param name="list">The list object reference.</param>
    <returns>The collection object reference.</returns>
  </GetCollection>
  <GetEnumerator>
    <summary>
      Gets the Collection Enumerator.
    </summary>
    <returns>The Collection Enumerator.</returns>
  </GetEnumerator>
  <!-- Custom Methods -->
  <GetIndex>
    <summary>
      Get index from data value.
    </summary>
    <param name="dbColumn">The DbColumn data value.</param>
    <returns>The item index.</returns>
  </GetIndex>
  <GetDescription>
    <summary>
      Gets the Item Description.
    </summary>
    <param name="dbColumn">The DbColumn data value.</param>
    <returns>The Item Description.</returns>
  </GetDescription>
  <GetItem>
    <summary>
      Gets the ControlItem with the supplied index.
    </summary>
    <param name="dbColumn">The DbColumn data value.</param>
    <returns>The selected ControlItem.</returns>
  </GetItem>
  <GetItems>
    <summary>
      Gets the Items with the supplied PropertyName value.
    </summary>
    <param name="dbColumn">The DbColumn data value.</param>
    <returns>The valid ControlItems.</returns>
  </GetItems>
  <!-- Collection Properties -->
  <Indexer>
    <summary>
      Gets the item by index value.
    </summary>
    <param name="index">The index value.</param>
    <returns>The element item object.</returns>
  </Indexer>
</items>