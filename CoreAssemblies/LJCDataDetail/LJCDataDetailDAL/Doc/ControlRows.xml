<!-- ControlRows.xml -->
<items>
  <CopyConstructor>
    <summary>
      The Copy constructor.
    </summary>
    <param name="items"></param>
  </CopyConstructor>
  
  <!-- Collection Methods -->
  <Add>
    <summary>
      Creates and adds the object from the provided values.
    </summary>
    <param name="id"></param>
    <param name="controlColumnID"></param>
    <param name="dataValueName"></param>
    <returns></returns>
  </Add>
  
  <!-- Search and Sort Methods -->
  <LJCSearchID>
    <summary>
      Retrieve the collection element.
    </summary>
    <param name="id"></param>
    <returns></returns>
  </LJCSearchID>
  <LJCSearchUnique>
    <summary>
      Retrieve the collection element with name.
    </summary>
    <param name="controlColumnID"></param>
    <param name="dataValueName"></param>
    <returns></returns>
  </LJCSearchUnique>
</items>