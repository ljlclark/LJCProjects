<!-- ControlRowManager.xml -->
<items>
  <!-- Custom Load/Retrieve Methods -->
  <LoadWithParentID>
    <summary>
      Loads the parent records.
    </summary>
    <param name="controlColumnID">The ControlRow parent ID.</param>
    <returns>The ControlColumns related to the parent.</returns>
  </LoadWithParentID>
  <RetrieveWithUnique>
    <summary>
      Retrieves a record with the supplied name value.
    </summary>
    <param name="controlColumnID"></param>
    <param name="rowIndex"></param>
    <param name="propertyNames"></param>
    <returns></returns>
  </RetrieveWithUnique>
  
  <!-- GetKey Methods -->
  <GetParentKey>
    <summary>
      Gets the Parent ID key columns.
    </summary>
    <param name="controlColumnID">The parent ID value.</param>
    <returns>The Parent KeyColumns object.</returns>
  </GetParentKey>
  <GetUniqueKey>
    <summary>
      Gets the ID key columns.
    </summary>
    <param name="controlColumnID"></param>
    <param name="rowIndex"></param>
    <returns></returns>
  </GetUniqueKey>
</items>