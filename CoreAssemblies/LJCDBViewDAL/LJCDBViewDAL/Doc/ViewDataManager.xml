<!-- ViewDataManager.xml -->
<items>
  <ViewDataManager>
    <summary>
      Provides ViewData specific data manipulation methods.
    </summary>
  </ViewDataManager>
  <ViewDataManagerC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="dbServiceRef">The database service reference object.</param>
    <param name="dataConfigName">The data configuration name.</param>
    <param name="tableName">The table name.</param>
  </ViewDataManagerC>
  <RetrieveWithUniqueKey>
    <summary>
      Retrieves the record by the unique key.
    </summary>
    <param name="viewTableID">The ViewTable ID value.</param>
    <param name="name">The Name value.</param>
    <returns>The data object record.</returns>
  </RetrieveWithUniqueKey>
  <LoadWithParentId>
    <summary>
      Retrieves a collection of Data records for the specified parent ID.
    </summary>
    <param name="parentID">The parent ID.</param>
    <returns>The data object record collection.</returns>
  </LoadWithParentId>
  <ResultWithParentID>
    <summary>
      Retrieves the DbResult set of data rows.
    </summary>
    <param name="parentID">The parent ID.</param>
    <returns>The DbResult rows.</returns>
  </ResultWithParentID>
  <SaveData>
    <summary>
      Updates the record if it exists, otherwise creates it.
    </summary>
    <param name="viewData">The data record.</param>
    <returns>true if successful; otherwise, false.</returns>
  </SaveData>
  <AddData>
    <summary>
      Adds a ViewData record.
    </summary>
    <param name="viewData">The data record.</param>
    <returns>The added ViewData record with the assigned ID value.</returns>
  </AddData>
</items>