<!-- ViewConditionManager.xml -->
<items>
  <ViewConditionManager>
    <summary>
      Provides ViewCondition specific data manipulation methods.
    </summary>
  </ViewConditionManager>
  <ViewConditionManagerC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="dbServiceRef">The database service reference object.</param>
    <param name="dataConfigName">The data configuration name.</param>
    <param name="tableName">The table name.</param>
  </ViewConditionManagerC>
  <RetrieveWithUniqueKey>
    <summary>
      Retrieves the record by the unique key.
    </summary>
    <param name="viewConditionSetID">The ViewConditionSet ID.</param>
    <param name="firstValue">The first value.</param>
    <returns>The data object record.</returns>
  </RetrieveWithUniqueKey>
  <LoadWithParentID>
    <summary>
      Retrieves a collection of Data records for the specified parent ID.
    </summary>
    <param name="conditionSetID">The parent ID.</param>
    <returns>The data object record collection.</returns>
  </LoadWithParentID>
  <ResultWithParentID>
    <summary>
      Retrieves the DbResult set of data rows.
    </summary>
    <param name="conditionSetID">The parent ID.</param>
    <returns>The DbResult rows.</returns>
  </ResultWithParentID>
  <SaveData>
    <summary>
      Updates the record if it already exists, otherwise creates it.
    </summary>
    <param name="viewCondition">The data record.</param>
    <returns>True if successful otherwise, false.</returns>
  </SaveData>
  <AddData>
    <summary>
      Adds a data record.
    </summary>
    <param name="viewCondition">The data record.</param>
    <returns>The added data object record with the assigned ID value.</returns>
  </AddData>
</items>