<!-- XMLBuilder.xml -->
<items>
  <ToString>
    <summary>
      Retrieves the XML text.
    </summary>
    <returns>The XML text.</returns>
  </ToString>
  <Text>
    <summary>
      Adds a newline if line length is greater than LineLimit.
    </summary>
    <param name="text"></param>
    <returns></returns>
  </Text>
  <BeginElement>
    <summary>
      Creates the element begin tag.
    </summary>
    <param name="name">The element name.</param>
    <param name="text">The element text.</param>
    <param name="xmlAttributes">The element attributes.</param>
    <param name="isIndented">Indicates if the element should be indented.</param>
    <returns>The element begin tag.</returns>
  </BeginElement>
  <CreateElement>
    <summary>
      Creates an XML element.
    </summary>
    <param name="name">The element name.</param>
    <param name="text">The element text.</param>
    <param name="xmlAttributes">The element attributes.</param>
    <param name="isIndented">Indicates if the element should be indented.</param>
    <param name="close">Indicates if the element will be closed.</param>
    <returns>The XML element.</returns>
  </CreateElement>
  <EndElement>
    <summary>
      Creates the element end tag.
    </summary>
    <param name="name">The element name.</param>
    <param name="isIndented">Indicates if the element should be indented.</param>
    <returns>The element end tag.</returns>
  </EndElement>
  <StartAttributes>
    <summary>
      Creates the XML start attributes.
    </summary>
    <returns>The XML start attributes.</returns>
  </StartAttributes>
</items>