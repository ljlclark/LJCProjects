<!-- HTMLData.xml -->
<items>
  <!-- Static DataTable Functions -->
  <DataTableHeadings>
    <summary>
      Create table headings from DataTable.
    </summary>
    <param name="dataTable"></param>
    <returns></returns>
  </DataTableHeadings>
  <DataTableHTML>
    <summary>
      Creates an HTML table from DataTable.
    </summary>
    <param name="dataTable"></param>
    <param name="maxRows"></param>
    <returns></returns>
  </DataTableHTML>
  <DataTableRows>
    <summary>
      Creates table rows from DataTable.
    </summary>
    <param name="dataTable"></param>
    <param name="maxRows"></param>
    <returns></returns>
  </DataTableRows>
  <!-- Static Data Object Functions -->
  <DataHeadings>
    <summary>
      Create table headings from a Data Object.
    </summary>
    <param name="dataObjects"></param>
    <param name="propertyNames"></param>
    <returns></returns>
  </DataHeadings>
  <DataHTML>
    <summary>
      Create an HTML table from a Data Object.
    </summary>
    <param name="dataObjects"></param>
    <param name="propertyNames"></param>
    <param name="maxRows"></param>
    <returns></returns>
  </DataHTML>
  <DataRows>
    <summary>
      Create table rows from a Data Object.
    </summary>
    <param name="dataObjects"></param>
    <param name="propertyNames"></param>
    <param name="maxRows"></param>
    <returns></returns>
  </DataRows>
  <!-- Static DbResult Functions -->
  <ResultHeadings>
    <summary>
      Create table headings from result.
    </summary>
    <param name="dbResult"></param>
    <returns></returns>
  </ResultHeadings>
  <ResultHTML>
    <summary>
      Creates an HTML table from DbResult.
    </summary>
    <param name="dbResult"></param>
    <param name="textState"></param>"
    <param name="maxRows"></param>
    <returns></returns>
  </ResultHTML>
  <ResultRows>
    <summary>
      Create table rows from result.
    </summary>
    <param name="dbResult"></param>
    <param name="maxRows"></param>
    <returns></returns>
  </ResultRows>
</items>