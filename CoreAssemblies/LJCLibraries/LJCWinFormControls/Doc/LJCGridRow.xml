<!-- LJCGridRow.xml -->
<items>
  <LJCRowSetValues2>
    <summary>
      Updates a grid row with the DbValues.
    </summary>
    <param name="grid">The grid control.</param>
    <param name="dbValues">The result values.</param>
  </LJCRowSetValues2>
  <LJCRowSetValues1>
    <summary>
      Updates a grid row with the object values.
    </summary>
    <param name="grid">The grid control.</param>
    <param name="values">The object values.</param>
  </LJCRowSetValues1>
  <LJCRowSetValues>
    <summary>
      Updates a grid row with the record values.
    </summary>
    <param name="grid">The grid control.</param>
    <param name="record">The record object.</param>
  </LJCRowSetValues>

  <!-- Saved Value Methods -->
  <LJCSetInt321>
    <summary>
      Stores an int key and int value pair.
    </summary>
    <param name="key">The int key.</param>
    <param name="value">The int value.</param>
  </LJCSetInt321>
  <LJCSetInt322>
    <summary>
      Stores a string key and int value pair.
    </summary>
    <param name="key">The string key.</param>
    <param name="value">The int value.</param>
  </LJCSetInt322>

  <LJCSetInt641>
    <summary>
      Stores a long key and long value pair.
    </summary>
    <param name="key">The long key.</param>
    <param name="value">The long value.</param>
  </LJCSetInt641>
  <LJCSetInt642>
    <summary>
      Stores a string key and long value pair.
    </summary>
    <param name="key">The string key.</param>
    <param name="value">The long value.</param>
  </LJCSetInt642>
  
  <LJCSetString1>
    <summary>
      Stores a int key and string value pair.
    </summary>
    <param name="key">The int key.</param>
    <param name="value">The string value.</param>
  </LJCSetString1>
  <LJCSetString2>
    <summary>
      Stores a string key and string value pair.
    </summary>
    <param name="key">The string key.</param>
    <param name="value">The string value.</param>
  </LJCSetString2>
  
  <LJCGetInt321>
    <summary>
      Gets the stored int value using an int key.
    </summary>
    <param name="key">The int key.</param>
    <returns>The stored int value.</returns>
  </LJCGetInt321>
  <LJCGetInt322>
    <summary>
      Gets the stored int value using a string key.
    </summary>
    <param name="key">The string key.</param>
    <returns>The stored int value.</returns>
  </LJCGetInt322>

  <LJCGetInt641>
    <summary>
      Gets the stored long value using a long key.
    </summary>
    <param name="key">The long key.</param>
    <returns>The stored long value.</returns>
  </LJCGetInt641>
  <LJCGetInt642>
    <summary>
      Gets the stored long value using a string key.
    </summary>
    <param name="key">The string key.</param>
    <returns>The stored long value.</returns>
  </LJCGetInt642>
  
  <LJCGetString1>
    <summary>
      Gets the stored string value using an int key.
    </summary>
    <param name="key">The int key.</param>
    <returns>The stored string value.</returns>
  </LJCGetString1>
  <LJCGetString2>
    <summary>
      Gets the stored string value using a string key.
    </summary>
    <param name="key">The string key.</param>
    <returns>The stored string value.</returns>
  </LJCGetString2>

  <!-- Cell Methods -->
  <LJCGetCellText>
    <summary>
      Sets the cell value.
    </summary>
    <param name="columnName">The column name.</param>
  </LJCGetCellText>
  <LJCSetCellText>
    <summary>
      Sets the cell value by name.
    </summary>
    <param name="columnName">The column name.</param>
    <param name="value">The cell value.</param>
  </LJCSetCellText>
  <LJCSetCellText1>
    <summary>
      Sets the cell value by index.
    </summary>
    <param name="index">The column index.</param>
    <param name="value">The cell value.</param>
  </LJCSetCellText1>
</items>
