<!-- DataType.xml -->
<items>
  <DataType>
    <summary>
      Represents the Class/Type documentation object.
    </summary>
  </DataType>
  <DataTypeC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="dataAssembly">The DataAssembly object.</param>
    <param name="typeMember">The Type member object.</param>
    <param name="assemblyReflect">The LJCAssemblyReflect object.</param>
  </DataTypeC>
  <CreateMethodsData>
    <summary>
      Creates the child methods data.
    </summary>
    <param name="dataAssembly">The DataAssembly object.</param>
  </CreateMethodsData>
  <CreatePropertiesData>
    <summary>
      Creates the child properties data.
    </summary>
    <param name="dataAssembly">The DataAssembly object.</param>
  </CreatePropertiesData>
  <CreateFieldsData>
    <summary>
      Creates the child fields data.
    </summary>
    <param name="dataAssembly">The DataAssembly object.</param>
  </CreateFieldsData>
  <Doc>
    <summary>Gets or sets the Doc value.</summary>
    <remarks>
      The Doc object contains the deserialized documentation XML data.
    </remarks>
  </Doc>
  <TypeMember>
    <summary>Gets or sets the TypeMember value.</summary>
    <remarks>
      This DocMember object contains the deserialized type documentation
      XML data.
    </remarks>
  </TypeMember>
</items>