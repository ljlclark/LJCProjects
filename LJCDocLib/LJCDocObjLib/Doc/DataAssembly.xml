<?xml version="1.0" encoding="utf-8" ?>
<!-- DataAssembly.xml -->
<items>
  <DataAssembly>
    <summary>
      Represents the Assembly documentation data.
    </summary>
    <remarks>
      <para>
        The Assembly data object contain the list of class/type data objects.
      </para>
    </remarks>
  </DataAssembly>
  <DataAssemblyC>
    <summary>
      Initializes an object instance.
    </summary>
    <param name="dataRoot">The DataRoot object.</param>
    <param name="xmlFileName">The XML file name.</param>
    <param name="description">The description value.</param>
    <param name="mainImage">The main image value.</param>
  </DataAssemblyC>
  <CreateTypesData>
    <summary>
      Creates the child class/type data.
    </summary>
  </CreateTypesData>
  <GetAssemblyReflect>
    <summary>
      Get the AssemblyReflect object.
    </summary>
    <param name="xmlFileSpec">The documentation XML file specification.</param>
  </GetAssemblyReflect>
  <Doc>
    <summary>Gets or sets the Doc value.</summary>
    <remarks>
      The Doc object contains the deserialized documentation XML data.
    </remarks>
  </Doc>
</items>