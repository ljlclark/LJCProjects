<?xml version="1.0" encoding="utf-8" ?>
<!-- DocGenGroupManager.xml -->
<items>
  <DocGenGroupManager>
    <summary>
      Provides DocGenGroup specific data manipulation methods.
    </summary>
  </DocGenGroupManager>
  <RetrieveWithName>
    <summary>
      Retrieves a data record with the supplied value.
    </summary>
    <param name="name">The DocGenGroup name.</param>
    <returns>The data record.</returns>
  </RetrieveWithName>
  <RetrieveAssemblyWithName>
    <summary>
      Retrieves a data record with the supplied values.
    </summary>
    <param name="docGroupName">The DocGenGroup name.</param>
    <param name="docAssemblyName">The DocGenAssembly name.</param>
    <returns>The DocGenAssembly object.</returns>
  </RetrieveAssemblyWithName>
  <Load>
    <summary>
      Loads a collection of data records from the XML file.
    </summary>
    <param name="xmlFileName">The XML file name.</param>
    <returns>The collection of data records.</returns>
  </Load>
  <LoadAssemblies>
    <summary>
      Loads the group assembly data records.
    </summary>
    <param name="groupName">The group name.</param>
    <returns>The collection of data records.</returns>
  </LoadAssemblies>
  <Save>
    <summary>
      Saves the updated XML file.
    </summary>
    <param name="xmlFileName">The XML file name.</param>
    <returns>True if the file saved successfully, otherwise false.</returns>
  </Save>
  <DeleteGroup>
    <summary>
      Deletes a DocGenGroup and all associated DocGenAssemblies.
    </summary>
    <param name="keyObject">The key record object.</param>
  </DeleteGroup>
  <DeleteAssembly>
    <summary>
      Deletes a DocGenAssembly entry.
    </summary>
    <param name="docGenGroupName">The DocGenGroup name.</param>
    <param name="keyObject">The key record object.</param>
  </DeleteAssembly>
</items>