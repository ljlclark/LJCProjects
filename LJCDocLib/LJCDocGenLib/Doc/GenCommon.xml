<!-- GenCommon.xml -->
<items>
  <GenCommon>
    <summary>
      The static common DocGen methods.
    </summary>
  </GenCommon>
  <Syntax1>
    <summary>
      Retrieves the Syntax remark paragraph if it exists.
    </summary>
    <param name="remark">The remarks object.</param>
    <returns>The HTML Syntax section.</returns>
    <include path='items/GenCommon/*' file='Doc/GenCommon.xml'/>
  </Syntax1>
  <Syntax2>
    <summary>
      Creates the Syntax XML string from the supplied text.
    </summary>
    <param name="text">The Syntax string.</param>
    <returns>The Syntax XML string.</returns>
  </Syntax2>
  <Summary>
    <summary>
      Retrieves the Summary text if it exists.
    </summary>
    <param name="location">The item location.</param>
    <param name="summary">The summary value.</param>
    <returns>The HTML Summary section.</returns>
  </Summary>
  <Returns>
    <summary>
      Retrieves the Returns text if it exists.
    </summary>
    <param name="location">The item location.</param>
    <param name="returns">The returns value.</param>
    <returns>The HTML Returns section.</returns>
  </Returns>
  <Remarks>
    <summary>
      Retrieves the remarks text if it exists.
    </summary>
    <param name="location">The item location.</param>
    <param name="remark">The remarks object.</param>
    <returns>The HTML Remarks section.</returns>
  </Remarks>
  <Example>
    <summary>
      Retrieves the Example section if it exists.
    </summary>
    <param name="example">The example object.</param>
    <returns>The HTML Example section.</returns>
  </Example>
</items>