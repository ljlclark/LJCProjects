<?xml version="1.0" encoding="utf-8" ?>
<!-- ControlColumnManager.xml -->
<items>
  <!-- Load/Retrieve Methods -->
  <LoadWithParentID>
    <summary>
      Loads the child records.
    </summary>
    <param name="controlTabID">The ControlColumns parent ID.</param>
    <returns>The ControlColumns related to the parent.</returns>
  </LoadWithParentID>
  <RetrieveWithUnique>
    <summary>
      Retrieves a record with the supplied values.
    </summary>
    <param name="controlTabID"></param>
    <param name="columnIndex"></param>
    <param name="propertyNames"></param>
    <returns></returns>
  </RetrieveWithUnique>
  
  <!-- GetKey Methods -->
  <GetParentKey>
    <summary>
      Gets the Parent ID key columns.
    </summary>
    <param name="controlTabID">The parent ID value.</param>
    <returns>The Parent KeyColumns object.</returns>
  </GetParentKey>
  <GetUniqueKey>
    <summary>
      Gets the unique key columns.
    </summary>
    <param name="controlTabID"></param>
    <param name="columnIndex"></param>
    <returns></returns>
  </GetUniqueKey>
</items>