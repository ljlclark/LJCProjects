<!-- ChangedNames.xml -->
<items>
  <Add>
    <summary>
      Adds the property name to the list if the value has changed and it
      is not already in the list.
    </summary>
    <typeparam name="T">The Type of the value to add to the list.</typeparam>
    <param name="propertyName">The property name.</param>
    <param name="oldValue">The previous value.</param>
    <param name="newValue">The new value.</param>
    <returns>The new value.</returns>
  </Add>
  <FindName>
    <summary>
      Returns the existing property name or null if it does not exist.
    </summary>
    <param name="propertyName">The property name.</param>
    <returns>The existing property name.</returns>
  </FindName>
</items>
